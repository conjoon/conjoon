{
    "name": "@conjoon/conjoon",
    "product": "ext",
    "version": "2.0.0-alpha.0",
    "description": "JavaScript email client powered by Sencha ExtJS and coon.js.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/conjoon/conjoon.git"
    },
    "keywords": [
        "webmail",
        "sencha",
        "extjs",
        "l8js",
        "l8",
        "coonjs",
        "conjoon",
        "imap",
        "material"
    ],
    "author": "Thorsten Suckow-Homberg <thorsten@suckow-homberg.de>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/conjoon/conjoon/issues"
    },
    "homepage": "https://github.com/conjoon/conjoon#readme",
    "scripts": {
        "start": "npm run dev",
        "clean": "rimraf build",
        "dev": "webpack-dev-server --env.profile=desktop --env.browser=yes --env.verbose=no --env.cmdopts=--uses",
        "preprodbuild": "npx delorean -p -c ./.deloreanrc.build.json",
        "postprodbuild": "npx delorean -r -c ./.deloreanrc.build.json",
        "build": "npm run preprodbuild && npm run clean && npm run build:extjs && npm run postprodbuild",
        "build:extjs": "cross-env webpack --env.profile=desktop --env.environment=production --env.treeshake=yes --env.cmdopts=--uses",
        "stage": "webpack-dev-server --config ./webpack.prod.config.js --open",
        "build:dev": "npm run build:test && npx node-git-hooks",
        "build:test": "npm i --save-dev && npx extjs-link && npx siesta-lib-helper",
        "build:staging": "npm run build && npm run stage",
        "build:testing": "npm run clean && cross-env webpack --env.profile=desktop --env.treeshake=yes --env.cmdopts=--testing",
        "test": "npm run test:siesta",
        "postversion": "git push && git push --tags",
        "release": "npm run preversion && standard-version --commit-all",
        "preversion": "npx delorean -r",
        "release:patch": "npm run preversion && standard-version --commit-all --release-as patch && npm run postversion",
        "release:minor": "npm run preversion && standard-version --commit-all --release-as minor && npm run postversion",
        "release:major": "npm run preversion && standard-version --commit-all --release-as major && npm run postversion",
        "deps:prod:local": "npm i -P @l8js/l8@file:../../@l8js/l8 @coon-js/extjs-comp-navport@file:../../@coon-js/extjs-comp-navport @conjoon/extjs-theme-material@file:../extjs-theme-material @coon-js/extjs-lib-comp@file:../../@coon-js/extjs-lib-comp @conjoon/extjs-app-webmail@file:../extjs-app-webmail @conjoon/extjs-dev-webmailsim@file:../extjs-dev-webmailsim @conjoon/extjs-app-localmailaccount@file:../extjs-app-localmailaccount",
        "deps:prod:latest": "npm i -P @l8js/l8@latest @coon-js/extjs-comp-navport@latest @conjoon/extjs-theme-material@latest @conjoon/extjs-app-imapuser@latest @conjoon/extjs-app-webmail@latest @conjoon/extjs-dev-webmailsim@latest @conjoon/extjs-app-localmailaccount@latest @coon-js/extjs-lib-comp@latest @coon-js/extjs-lib-core@latest",
        "test:siesta": "npx ws --port 8099 --static.index tests.redirect.html --open",
        "update:app.json": "node bin/updateAppJson.mjs"
    },
    "standard-version": {
        "scripts": {
            "postbump": "npm run update:app.json && git add ./app.json"
        }
    },
    "devDependencies": {
        "@babel/cli": "^7.19.3",
        "@babel/core": "^7.20.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
        "@babel/plugin-transform-async-to-generator": "^7.5.0",
        "@babel/plugin-transform-regenerator": "^7.4.5",
        "@babel/preset-env": "^7.20.2",
        "@commitlint/cli": "^13.2.0",
        "@commitlint/config-conventional": "^13.2.0",
        "@coon-js/delorean": "^0.3.3",
        "@coon-js/extjs-package-loader": "^0.1.10",
        "@coon-js/extjs-plug-themeutil": "^0.1.3",
        "@coon-js/siesta-lib-helper": "^0.1.16",
        "@sencha/cmd": "^7.6.0",
        "@sencha/ext": "^7.4.0",
        "@sencha/ext-classic": "^7.4.0",
        "@sencha/ext-classic-runtime": "^7.5.0",
        "@sencha/ext-classic-theme-material": "^7.4.0",
        "@sencha/ext-modern": "^7.6.0",
        "@sencha/ext-modern-runtime": "^7.5.0",
        "@sencha/ext-ux": "^7.4.0",
        "@sencha/ext-webpack-plugin": "^7.4.0",
        "babel-loader": "^8.0.6",
        "babel-plugin-add-module-exports": "^1.0.2",
        "babel-preset-minify": "^0.5.2",
        "copy-webpack-plugin": "^6.4.1",
        "cross-env": "^5.2.0",
        "eslint": "^7.32.0",
        "local-web-server": "^5.2.0",
        "lodash.find": "^4.6.0",
        "node-git-hooks": "^1.0.6",
        "portfinder": "^1.0.21",
        "replace": "^1.1.1",
        "siesta-lite": "^5.5.2",
        "standard-version": "^9.3.1",
        "uninstall": "^0.0.0",
        "webpack": "^4.39.2",
        "webpack-cli": "^3.3.6",
        "webpack-dev-server": "~3.8.0"
    },
    "dependencies": {
        "@conjoon/extjs-app-imapuser": "^1.0.2",
        "@conjoon/extjs-app-localmailaccount": "^1.0.2",
        "@conjoon/extjs-app-webmail": "^2.0.0-alpha.0",
        "@conjoon/extjs-dev-imapusersim": "^1.0.5",
        "@conjoon/extjs-dev-webmailsim": "^2.0.0-alpha.0",
        "@conjoon/extjs-theme-material": "^0.3.1",
        "@coon-js/extjs-app-user": "^0.1.7",
        "@coon-js/extjs-comp-navport": "^0.2.4",
        "@coon-js/extjs-ctrl-simmanager": "^0.2.4",
        "@coon-js/extjs-lib-comp": "^1.0.1",
        "@coon-js/extjs-lib-core": "^1.1.1",
        "@l8js/l8": "^0.12.1",
        "t": "^0.5.1"
    },
    "engines": {
        "node": "16.14.2"
    }
}
